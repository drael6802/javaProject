<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>SEARCH_MENU</title>
<style type="text/css">
	.searchDiv {
		margin: auto;
		border: 3px solid aqua;
		width: 300px;
		height: 30px;
	}
	.searchInput {
		border: none;
		width: 230px;
		height: 30px;
		background: none;
	}
	.searchReset {
		border: none;
		background: none;
		width: 20px;
		height: 30px;
	}
	.searchSubmit {
		border: none;
		background: none;
		width: 30px;
		height: 30px;
	}
</style>
</head>
<body>
<form action="">
	<div style="height: 30px;"></div>
	<div class="searchDiv">
		<input type="text" id="" name="" class="searchInput"/>
		<input type="reset" value="X" class="searchReset"/>
		<input type="submit" value="검색" class="searchSubmit"/>
	</div>
</form>
</body>
</html>